<process name="EBS_SalesOrder_ConcProg_BPELProcess"
         targetNamespace="http://xmlns.oracle.com/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/EBS_SalesOrder_ConcProg_BPELProcess"
         xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:client="http://xmlns.oracle.com/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/EBS_SalesOrder_ConcProg_BPELProcess"
         xmlns:ora="http://schemas.oracle.com/xpath/extension"
         xmlns:ui="http://xmlns.oracle.com/soa/designer"
         xmlns:bpelx="http://schemas.oracle.com/bpel/extension"
         xmlns:bpel="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
         xmlns:ns1="http://xmlns.oracle.com/pcbpel/adapter/apps/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/InsertSalesOrder"
         xmlns:ns2="http://xmlns.oracle.com/pcbpel/adapter/apps/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/ImportSalesOrderCP"
         xmlns:ns3="http://xmlns.oracle.com/pcbpel/adapter/file/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/getSalesOrderDetails"
         xmlns:ns4="http://xmlns.oracle.com/pcbpel/adapter/file/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/getConcProgDetails"
         xmlns:ns5="http://xmlns.oracle.com/pcbpel/adapter/db/top/InsertSalesOrder"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns:ns6="http://xmlns.oracle.com/pcbpel/adapter/db/sp/ImportSalesOrderCP">

    <import ui:processWSDL="true" namespace="http://xmlns.oracle.com/SOATutorialsWin11/EBS_SalesOrder_Import_ConcProg/EBS_SalesOrder_ConcProg_BPELProcess" location="../WSDLs/EBS_SalesOrder_ConcProg_BPELProcess.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        PARTNERLINKS                                                      
        List of services participating in this BPEL process               
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    --> 
    <partnerLinks>
        <!-- 
      The 'client' role represents the requester of this service. It is 
      used for callback. The location and correlation information associated
      with the client role are automatically set using WS-Addressing.
    -->
        <partnerLink name="ebs_salesorder_concprog_bpelprocess_client" partnerLinkType="client:EBS_SalesOrder_ConcProg_BPELProcess" myRole="EBS_SalesOrder_ConcProg_BPELProcessProvider" partnerRole="EBS_SalesOrder_ConcProg_BPELProcessRequester"/>
        <partnerLink name="InsertSalesOrder" partnerLinkType="ns1:InsertSalesOrder_plt"
                     partnerRole="InsertSalesOrder_role"/>
        <partnerLink name="ImportSalesOrderCP" partnerLinkType="ns2:ImportSalesOrderCP_plt"
                     partnerRole="ImportSalesOrderCP_role"/>
        <partnerLink name="getSalesOrderDetails" partnerLinkType="ns3:SynchRead_plt" partnerRole="SynchRead_role"/>
        <partnerLink name="getConcProgDetails" partnerLinkType="ns4:SynchRead_plt" partnerRole="SynchRead_role"/>
    </partnerLinks>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
        VARIABLES                                                        
        List of messages and XML documents used within this BPEL process 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <variables>
        <!-- Reference to the message passed as input during initiation -->
        <variable name="inputVariable" messageType="client:EBS_SalesOrder_ConcProg_BPELProcessRequestMessage"/>

        <!-- Reference to the message that will be sent back to the requester during callback -->
        <variable name="outputVariable" messageType="client:EBS_SalesOrder_ConcProg_BPELProcessResponseMessage"/>
        <variable name="getSalesOrderDetailsInvokeInputVariable" messageType="ns3:Empty_msg"/>
        <variable name="getSalesOrderDetailsInvokeOutputVariable" messageType="ns3:SynchRead_msg"/>
        <variable name="getConcProgDetailsInvokeInputVariable" messageType="ns4:Empty_msg"/>
        <variable name="getConcProgDetailsInvokeOutputVariable" messageType="ns4:SynchRead_msg"/>
        <variable name="InsertSalesOrderInvokeInputVariable" messageType="ns1:OeHeadersIfaceAllCollection_msg"/>
        <variable name="ImportSalesOrderCPInvokeInputVariable" messageType="ns2:args_in_msg"/>
    </variables>

    <!-- 
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
       ORCHESTRATION LOGIC                                               
       Set of activities coordinating the flow of messages across the    
       services integrated within this business process                  
      ////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
    -->
    <sequence name="main">
        <!-- Receive input from requestor. (Note: This maps to operation defined in EBS_SalesOrder_ConcProg_BPELProcess.wsdl) -->
        <receive name="receiveInput" partnerLink="ebs_salesorder_concprog_bpelprocess_client" portType="client:EBS_SalesOrder_ConcProg_BPELProcess" operation="process" variable="inputVariable" createInstance="yes"/>
        <invoke name="getSalesOrderDetailsInvoke" partnerLink="getSalesOrderDetails"
                portType="ns3:SynchRead_ptt" operation="SynchRead"
                inputVariable="getSalesOrderDetailsInvokeInputVariable"
                outputVariable="getSalesOrderDetailsInvokeOutputVariable" bpelx:invokeAsDetail="no"/>
        <assign name="SetSalesOrderDetailsAssign">
            <copy>
                <from>$getSalesOrderDetailsInvokeOutputVariable.body</from>
                <to>$InsertSalesOrderInvokeInputVariable.OeHeadersIfaceAllCollection</to>
            </copy>
        </assign>
        <invoke name="getConcProgDetailsInvoke" partnerLink="getConcProgDetails"
                portType="ns4:SynchRead_ptt" operation="SynchRead"
                inputVariable="getConcProgDetailsInvokeInputVariable"
                outputVariable="getConcProgDetailsInvokeOutputVariable" bpelx:invokeAsDetail="no"/>
        <invoke name="InsertSalesOrderInvoke" partnerLink="InsertSalesOrder"
                portType="ns1:InsertSalesOrder_ptt" operation="insert"
                inputVariable="InsertSalesOrderInvokeInputVariable" bpelx:invokeAsDetail="no"/>
        <assign name="SetConcProgDetailsAssign">
            <copy>
                <from>$getConcProgDetailsInvokeOutputVariable.body</from>
                <to>$ImportSalesOrderCPInvokeInputVariable.InputParameters</to>
            </copy>
        </assign>
        <invoke name="ImportSalesOrderCPInvoke" partnerLink="ImportSalesOrderCP"
                portType="ns2:ImportSalesOrderCP_ptt" operation="ImportSalesOrderCP"
                inputVariable="ImportSalesOrderCPInvokeInputVariable" bpelx:invokeAsDetail="no">
            <bpelx:toProperties>
                <bpelx:toProperty name="jca.apps.Username">'OPERATIONS'</bpelx:toProperty>
                <bpelx:toProperty name="jca.apps.Responsibility">'Order Management Super User, Vision Operations (USA)'</bpelx:toProperty>
                <bpelx:toProperty name="jca.apps.ORG_ID">204</bpelx:toProperty>
            </bpelx:toProperties>
        </invoke>
        <!-- 
          Asynchronous callback to the requester. (Note: the callback location and correlation id is transparently handled using WS-addressing.)
        -->
        <invoke name="callbackClient" partnerLink="ebs_salesorder_concprog_bpelprocess_client" portType="client:EBS_SalesOrder_ConcProg_BPELProcessCallback" operation="processResponse" inputVariable="outputVariable"/>
    </sequence>
</process>